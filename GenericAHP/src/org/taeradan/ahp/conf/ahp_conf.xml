<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE root SYSTEM "ahp_conf.dtd">

<!--
    Document   : ahp_conf.xml
    Created on : 27 mars 2009, 15:05
    Author     : Yves Dubromelle
    Description:
		This document is the configuration file of the org.taeradan.ahp package, a new generic Java implementation of the AHP algorithm.
		For syntax information, just have a look at the ahp_conf.dtd file.
-->

<root>
	<name>Choix d'un produit à traiter dans un WIP</name>
	<prefmatrix>
		<row><elt value="1"/><elt value="1/3"/><elt value="1/3"/><elt value="3"/><elt value="1"/></row>
		<row><elt value="3"/><elt value="1"/><elt value="1"/><elt value="9"/><elt value="3"/></row>
		<row><elt value="3"/><elt value="1"/><elt value="1"/><elt value="9"/><elt value="3"/></row>
		<row><elt value="1/3"/><elt value="1/9"/><elt value="1/9"/><elt value="1"/><elt value="1/3"/></row>
		<row><elt value="1"/><elt value="1/3"/><elt value="1/3"/><elt value="3"/><elt value="1"/></row>
	</prefmatrix>
	<criteria id="C1">
		<name>Type de produit</name>
		<prefmatrix>
			<row><elt value="1"/><elt value="1/3"/><elt value="1"/></row>
			<row><elt value="3"/><elt value="1"/><elt value="1"/></row>
			<row><elt value="1"/><elt value="1"/><elt value="1"/></row>
		</prefmatrix>
		<indicator id="C1I1">
			<name>Ligne en flow-shop ou en job-shop</name>
			<maximize/>
		</indicator>
		<indicator id="C1I2">
			<name>Produit principal ou de complément</name>
			<maximize/>
		</indicator>
		<indicator id="C1I3">
			<name>Produit pour assemblage ou de rechange</name>
			<maximize/>
		</indicator>
	</criteria>
	<criteria id="C2">
		<name>Progression du produit</name>
		<prefmatrix>
			<row><elt value="1"/><elt value="1"/></row>
			<row><elt value="1"/><elt value="1"/></row>
		</prefmatrix>
		<indicator id="C2I1">
			<name>Progression globale</name>
			<maximize/>
		</indicator>
		<indicator id="C2I2">
			<name>Progression locale</name>
			<maximize/>
		</indicator>
	</criteria>
	<criteria id="C3">
		<name>Marge restante</name>
		<prefmatrix>
			<row><elt value="1"/><elt value="1"/></row>
			<row><elt value="1"/><elt value="1"/></row>
		</prefmatrix>
		<indicator id="C3I1">
			<name>Marge restante</name>
			<minimize/>
		</indicator>
		<indicator id="C3I2">
			<name>Marge restante pondérée</name>
			<minimize/>
		</indicator>
	</criteria>
	<criteria id="C4">
		<name>Temps dans la file d'attente</name>
		<prefmatrix>
			<row><elt value="1"/></row>
		</prefmatrix>
		<indicator id="C4I1">
			<name>Temps d'attente</name>
			<maximize/>
		</indicator>
	</criteria>
	<criteria id="C5">
		<name>Charge de la ressource</name>
		<prefmatrix>
			<row><elt value="1"/></row>
		</prefmatrix>
		<indicator id="C5I1">
			<name>Charge de la ressource</name>
			<minimize/>
		</indicator>
	</criteria>
</root>
